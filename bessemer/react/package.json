{
  "name": "@bessemer/react",
  "type": "module",
  "version": "0.0.0",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    },
    "./*": [
      {
        "import": "./dist/*.js",
        "types": "./dist/*.d.ts"
      },
      {
        "import": "./dist/*/index.js",
        "types": "./dist/*/index.d.ts"
      }
    ]
  },
  "sideEffects": false,
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch --external=none",
    "test": "jest",
    "prettier-fix": "prettier --write ."
  },
  "dependencies": {
    "@bessemer/cornerstone": "*",
    "react": "19.0.0",
    "react-dom": "19.0.0",
    "server-only": "0.0.1",
    "rsc-env":"0.0.2",

    "@tanstack/react-query": "5.62.15",
    "react-hook-form": "7.54.2"
  },
  "devDependencies": {
    "@types/react": "19.0.3",
    "@types/react-dom": "19.0.0"
  }
}