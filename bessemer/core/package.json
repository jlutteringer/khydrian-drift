{
  "name": "@bessemer/core",
  "type": "module",
  "version": "0.0.0",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    },
    "./*": [
      {
        "import": "./dist/*.js",
        "types": "./dist/*.d.ts"
      },
      {
        "import": "./dist/*/index.js",
        "types": "./dist/*/index.d.ts"
      }
    ]
  },
  "sideEffects": false,
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch --external=none",
    "test": "jest",
    "prettier-fix": "prettier --write ."
  },
  "dependencies": {
    "@bessemer/framework": "*",
    "@tiptap/core": "2.11.0",
    "@tiptap/pm": "2.11.0",
    "@tiptap/html": "2.11.0",
    "@tiptap/starter-kit": "2.11.0",

    "next": "15.1.2"
  }
}