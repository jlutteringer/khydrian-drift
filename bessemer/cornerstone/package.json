{
  "name": "@bessemer/cornerstone",
  "type": "module",
  "version": "0.5.88",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "sideEffects": false,
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    },
    "./*": {
      "import": "./dist/*.js",
      "types": "./dist/*.d.ts"
    }
  },
  "scripts": {
    "build": "tsup && tsc --project tsconfig.build.json --emitDeclarationOnly",
    "test": "jest",
    "prettier-fix": "prettier --write .",
    "clean": "rm -rf dist && rm -rf .turbo"
  },
  "dependencies": {
    "@js-temporal/polyfill": "0.5.1",
    "@types/lodash-es": "4.17.12",
    "immer": "10.1.1",
    "lodash-es": "4.17.21",
    "minimatch": "10.0.1",
    "pino": "9.6.0",
    "type-fest": "4.32.0",
    "ulid": "2.3.0",
    "zod": "4.0.15"
  },
  "devDependencies": {
    "expect-type": "1.2.2",
    "pino-pretty": "13.0.0"
  },
  "files": [
    "dist"
  ]
}
