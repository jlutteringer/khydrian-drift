{
  "name": "@bessemer/cornerstone",
  "type": "module",
  "version": "0.0.0",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "sideEffects": false,
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    },
    "./*": {
      "import": "./dist/*.js",
      "types": "./dist/*.d.ts"
    }
  },
  "scripts": {
    "build-turbo": "tsup",
    "dev-turbo": "tsup --watch --external=none",
    "test": "jest",
    "prettier-fix": "prettier --write .",
    "clean": "rm -rf dist && rm -rf .turbo"
  },
  "dependencies": {
    "immer": "10.1.1",
    "lodash-es": "4.17.21",
    "pino": "9.6.0"
  },
  "devDependencies": {
    "@types/lodash-es": "4.17.12",
    "pino-pretty": "13.0.0"
  }
}
