{
  "name": "@bessemer/zodios",
  "type": "module",
  "version": "0.5.68",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "sideEffects": false,
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    },
    "./*": {
      "import": "./dist/*.js",
      "types": "./dist/*.d.ts"
    }
  },
  "scripts": {
    "build": "tsup && tsc --project tsconfig.build.json --emitDeclarationOnly",
    "test": "jest",
    "prettier-fix": "prettier --write .",
    "clean": "rm -rf dist && rm -rf .turbo"
  },
  "devDependencies": {
    "@bessemer/cornerstone": "workspace:*",
    "@types/express": "4.17.19",
    "@types/multer": "1.4.8",
    "axios": "1.9.0",
    "express": "5.1.0",
    "form-data": "4.0.0",
    "multer": "1.4.5-lts.1",
    "rimraf": "5.0.5",
    "ts-node": "10.9.1"
  },
  "files": [
    "dist"
  ]
}
