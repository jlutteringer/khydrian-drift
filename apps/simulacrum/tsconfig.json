{
  "extends": "../../tsconfig.next.json",
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      "@bessemer/cornerstone": ["../../bessemer/cornerstone/src"],
      "@bessemer/cornerstone/*": ["../../bessemer/cornerstone/src/*"],
      "@bessemer/react": [ "../../bessemer/react/src"],
      "@bessemer/react/*": ["../../bessemer/react/src/*"],
      "@bessemer/mui": [ "../../bessemer/mui/src"],
      "@bessemer/mui/*": ["../../bessemer/mui/src/*"],
      "@bessemer/framework": ["../../bessemer/framework/src"],
      "@bessemer/framework/*": ["../../bessemer/framework/src/*"],
      "@bessemer/framework-next": ["../../bessemer/framework-next/src"],
      "@bessemer/framework-next/*": ["../../bessemer/framework-next/src/*"],
      "@bessemer/core": [ "../../bessemer/core/src"],
      "@bessemer/core/*": ["../../bessemer/core/src/*"],
      "@bessemer/foundry": ["../../bessemer/foundry/src"],
      "@bessemer/foundry/*": ["../../bessemer/foundry/src/*"],
      "@simulacrum": ["./"],
      "@simulacrum/*": ["./*"]
    },
  },
  "include": [
    "**/*.ts",
    "**/*.tsx",
    "next-env.d.ts",
    "next.config.js",
    ".next/types/**/*.ts"
  ],
  "exclude": [
    "node_modules"
  ]
}
